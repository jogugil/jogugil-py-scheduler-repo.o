apiVersion: v1
kind: Pod
metadata:
  generateName: cpu-heavy-pod-
  namespace: test-scheduler
  labels:
    app: cpu-heavy
spec:
  schedulerName: my-scheduler
  restartPolicy: Never
  containers:
  - name: cpu-heavy
    image: cpu-heavy:latest
    imagePullPolicy: Never
    resources:
      requests:
        memory: "512Mi"
        cpu: "100m"
      limits:
        memory: "1Gi"
        cpu: "200m"
